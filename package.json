{
    "name": "ics-to-tbcalresource",
    "version": "1.0.0",
    "description": "Simple utility for converting a trading/settlement calendar in ICS form to the equivalent Tbricks calendar resource (in XML format).",
    "private": true,
    "main": "dist/ics2tbricks.js",
    "type": "module",
    "engines": {
        "node": ">=20.0.0"
    },
    "dependencies": {
        "ics-to-json": "^2.0.0",
        "xml-js": "^1.6.11"
    },
    "devDependencies": {
        "@types/node": "^24.5.2",
        "typescript": "^5.9.3"
    },
    "scripts": {
        "clean": "rm -rf dist",
        "build": "npm run clean && tsc -p tsconfig.json",
        "start": "node dist/ics2tbricks.js",
        "typecheck": "tsc -p tsconfig.json --noEmit"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bgausden/ICS-to-TbricksCalResource.git"
    },
    "keywords": [
        "Tbricks",
        "ICS"
    ],
    "author": "Barry Gausden <barry@gausden.org> (http://twitter.com/bgausden)",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/bgausden/ICS-to-TbricksCalResource/issues"
    },
    "homepage": "https://github.com/bgausden/ICS-to-TbricksCalResource#readme"
}
